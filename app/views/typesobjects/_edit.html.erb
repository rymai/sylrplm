<fieldset>
	<legend>
		<%= t(:legend_typesobject) %>
	</legend>
	<%= form.label t(:label_object) %>
	<%= form.select(:object, @objectswithtype) %>
	<br/>
	<%= form.label t(:label_name) %>
	<%= form.text_field :name %>
	<br/>
	<%= form.label t(:label_description) %>
	<%= form.text_area :description %>
	<br/>
	<%= form.label t(:label_fields) %>
	<br/>
	<!--:<%= @typesobject.fields%>:<%= @typesobject.fields == "\"\""%>-->
	<%= render(
	:partial => "shared/form_json",
	:locals => {
	:fields => @typesobject.fields ,
	:form_id => form_id,
	:textarea_name => 'typesobject[fields]',
	:options => {"buttons" => "all"} }
	) %>
	<%= h_input_text(form ,:domain) %>
	<br />
</fieldset>