<% if object.have_lifecycle? == true && fonct!="edit_lifecycle" %>
<td> <%= link_to t("edit_lifecycle"), {:controller=>object.controller_name, :action=>"edit_lifecycle", :id => object.id} %> </td>
<% end %>

<td> <%= link_to t("new_#{object.modelname}"), {:controller=>object.controller_name, :action=>"new"} %></td>

<% if Controller.route_exists?(object.controller_name, "new_dup") && !object.id.nil? %>
<td> <%= link_to t("duplicate"), {:controller=>object.controller_name, :action=>"new_dup", :id => object.id} %> </td>
<% end %>

<% begin %>
<% if Controller.route_exists?(object.controller_name, "add_clipboard")%>
<% if logged_in?%>
<td><%= link_to  t("copy") , {:controller => object.controller_name, :action => "add_clipboard", :id => object.id}  , :remote=> :true%></td>
<% end %>
<% end %>
<% rescue Exception => e %>
<% end %>

<% if Controller.route_exists?(object.controller_name, "new_process")
begin %>
<td><%= link_to t("launchthis"), new_process_path(:definition_id => @definition.id) %></td>
<% rescue Exception=>e
end
end %>

